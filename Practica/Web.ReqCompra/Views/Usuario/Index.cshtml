</div>


<div id="modals"></div>

@section Scripts{
    <script>

        IndexUrl = function () {
             return {
                    urlGetUsuarios: "@Url.Action(nameof(UsuarioController.GetUsuarios), "Usuario")"
                }

        }();

    </script>
     <script type="text/javascript" src="~/js/Usuario/Index.js" asp-append-version="true"></script>

