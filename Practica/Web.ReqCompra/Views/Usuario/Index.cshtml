    <div>
        <a href="/Usuario/EditarUsuario/0" class="btn btn-outline-success btn-lg">Crear Usuario</a>
    </div>

</div>

<div class="">
    <div class="row">
        <div class="col-sm-12">
            <div id="divUsuario"></div>
