
@{
    ViewData["Title"] = "System";
}



<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <div id="divSector"></div>
            <div class="tab-pane fade show active" id="solicitud" role="tabpanel" aria-labelledby="solicitud-tab">
                <div class="mt-3">
                    <table id="tblSector" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    Identificador
                                </th>
                                <th>
                                    Descripción
                                </th>
                                <th>
                                    Código
                                </th>
                                 <th>
                                    Acción
                                </th>

                            </tr>
                        </thead>
                        <tbody>

                           @foreach (PropertiesSystemModel  System in ViewBag.tablaSystem)
                            {
                                <tr>
                                    <td>@System.Id</td>
                                    <td>@System.Nombre</td>
                                    <td>@System.Codigo</td>
                                    <td><a href="/System/EditarSystem/@System.Id"> <i class="fa fa-edit" aria-hidden="true"></i> </a></td>

                                </tr>

                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-sm-11">
            <button type="button" id="btnRegresarMantenedor" class="btn btn-primary">Regresar</button>
        </div>
    </div>
    <br />
</div>


<div id="modals"></div>

@section Scripts{
    <script>

        Index = function () {

            $(document).ready(function () {
                $('#tblSector').DataTable();
                $('#divIndicadoresEconomicos').css('display', 'none');
                $('#divIndicadoresEconomicos').hide();
            });

        }();

    </script>
    <script type="text/javascript" src="~/js/BackOffice/_EditarSystem.js" asp-append-version="true"></script>
}

